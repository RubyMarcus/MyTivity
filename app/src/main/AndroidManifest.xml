<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.apia22018.sportactivities">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/icon_on_bakgrund"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/icon_on_bakgrund"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning">

        <activity android:name=".screens.listActivities.ActivitiesActivity" />
        <activity android:name=".screens.SignUpActivity"></activity>
        <activity android:name=".screens.splash.SplashActivity"
            android:theme="@style/SplashTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".screens.detailActivities.DetailActivitiesActivity"/>

    </application>

</manifest>
